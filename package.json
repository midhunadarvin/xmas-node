{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build-prod": "npm run build:client && npm run -s build",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "test": "eslint src",
    "build:client": "webpack --env.production --config config/webpack",
    "start:client": "serve dist -s",
    "watch:client": "webpack-dev-server --config config/webpack"
  },
  "engines": {
    "node": "6.10.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.1",
    "axios": "^0.17.1",
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.22.0",
    "base-64": "^0.1.0",
    "body-parser": "^1.18.3",
    "compass": "^0.1.1",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "csv": "^2.0.0",
    "eslint": "^3.1.1",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "fs": "0.0.1-security",
    "ganalytics": "^2.0.1",
    "html-webpack-plugin": "^2.30.1",
    "linkstate": "^1.1.1",
    "material-components-web": "^0.26.0",
    "md-colors": "^1.0.0",
    "mongoose": "^4.13.17",
    "mongoose-relationship": "^0.1.5",
    "morgan": "^1.9.1",
    "net": "^1.0.2",
    "nodemailer": "^4.6.8",
    "preact": "^8.3.1",
    "preact-router": "^2.6.1",
    "request": "^2.88.0",
    "resource-router-middleware": "^0.6.0",
    "restful-mongoose": "^1.0.1",
    "style-loader": "^0.18.2",
    "tls": "0.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.22.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.11",
    "eslint": "^3.1.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^2.30.1",
    "net": "^1.0.2",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.6",
    "postcss-loader": "^2.1.6",
    "preact-material-components": "^1.5.4",
    "sass-loader": "^6.0.7",
    "serve": ">=7.0.0",
    "style-loader": "^0.18.2",
    "sw-precache-webpack-plugin": "^0.11.5",
    "tls": "0.0.1",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.3"
  }
}
